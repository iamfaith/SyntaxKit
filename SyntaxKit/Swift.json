{
	"name" : "Swift",
	"suffix" : "swift",
	"appearance" : "DefaultAppearanceDark",
	"features" :
	[
		{
	 "key" : "Label",
	 "pattern" : "(\\(|,)(\\s*)\\w+\\:",
	 "red" : 0.7,
	 "green" : 0.7,
	 "blue" : 0.7,
	 "color" : "SKColorLabelKey"
		},
		{
	 "key" : "NonLocalVariable",
	 "pattern" : "\\.\\w+\\s*",
	 "red" : 0.7,
	 "green" : 0.7,
	 "blue" : 0.7,
	 "color" : "SKColorInstanceVariableKey"
		},
		{
	 "key" : "FunctionCall",
	 "pattern" : "\\b\\w+\\s*\\(",
	 "red" : 0.7,
	 "green" : 0.7,
	 "blue" : 0.7,
	 "color" : "SKColorFunctionCallKey"
		},
		{
	 "key" : "Keyword",
	 "pattern" : "(\\b)(public|internal|private|override|func|var|let|lazy|init|deinit|required|convenience|mutating|nonmutating|static|class|struct|enum|switch|case|default|break|continue|return|if|else|guard|do|repeat|while|for|catch|try|throw|throws|rethrows|subscript|get|set|didSet|true|false|self|Self|dynamicType|nil|#selector|import|extension|in|where|is|as|super|infix|prefix|postfix|operator|associativity|precedence|((?<=associativity)\\s+(left|right)))(\\b)",
	 "red" : 0.8,
	 "green" : 0.0,
	 "blue" : 0.5,
	 "color" : "SKColorKeywordKey"
		},
		{
	 "key" : "SpecialCharacters",
	 "pattern" : ";|\\{|\\}|\\(|\\)|\\.|=|:|\\[|\\]|,|<|>|\\^\\{",
	 "red" : 0.7,
	 "green" : 0.7,
	 "blue" : 0.7,
	 "color" : "SKColorControlCharacterKey"
		},
		{
	 "key" : "Annotations",
	 "pattern" : "@(\\S+)",
	 "red" : 0.8,
	 "green" : 0.0,
	 "blue" : 0.5,
	 "color" : "SKColorKeywordKey"
		},
		{
	 "key" : "Number",
	 "pattern" : "\\b(0x|0b)?\\d+(\\.\\d+)?(f|L)?\\b",
	 "red" : 0.0,
	 "green" : 0.3,
	 "blue" : 0.8,
	 "color" : "SKColorNumberKey"
		},
		{
	 "key" : "ClosureArgument",
	 "pattern" : "\\b\\$\\d+\\b",
	 "red" : 0.7,
	 "green" : 0.7,
	 "blue" : 0.7,
	 "color" : "SKColorVariableKey"
		},
		{
	 "key" : "String",
	 "pattern" : "\"(?:[^\"\\\\]|\\\\.)*\"",
	 "red" : 0.9,
	 "green" : 0.0,
	 "blue" : 0.0,
	 "color" : "SKColorStringKey"
		},
		{
	 "key" : "StringSingleQuoted",
	 "pattern" : "'(?:[^'\\\\]|\\\\.)*'",
	 "red" : 0.9,
	 "green" : 0.0,
	 "blue" : 0.0,
	 "color" : "SKColorStringKey"
		},
		{
	 "key" : "Comment",
	 "pattern" : "//.*|/\\*(?:.|[\\n\\r])*?\\*/",
	 "red" : 0.0,
	 "green" : 0.8,
	 "blue" : 0.1,
	 "color" : "SKColorCommentKey"
		},
		{
	 "key" : "Documentation-Comment",
	 "pattern" : "///.*|/\\*\\*(?:.|[\\n\\r])*?\\*/",
	 "red" : 0.0,
	 "green" : 0.7,
	 "blue" : 0.6,
	 "color" : "SKColorDocumentationCommentKey"
		}
	 ],
	"completion" :
	[
		{
	 "name" : "Comment",
	 "description" : "A single line comment",
	 "search-tags" :
	 [
	  "comment",
	  "single line"
	  ],
	 "insertion-text" : "// ",
	 "scope" : "*"
		},
		{
	 "name" : "Multiline Comment",
	 "description" : "A multi line comment",
	 "search-tags" :
	 [
	  "comment",
	  "multi line"
	  ],
	 "insertion-text" : "/*  */",
	 "scope" : "*"
		}
	 ]
}